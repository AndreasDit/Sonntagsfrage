							"type": "LinkedServiceReference"
						}
					},
					{
						"name": "Clean_Data",
						"type": "AzureFunctionActivity",
						"dependsOn": [
							{
								"activity": "Crawl_Data",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
