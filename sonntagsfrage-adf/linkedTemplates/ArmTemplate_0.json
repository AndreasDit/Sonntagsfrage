				"typeProperties": {
					"subscriptionId": "[parameters('SonntagsfragePredictor_properties_typeProperties_subscriptionId')]",
					"resourceGroupName": "[parameters('SonntagsfragePredictor_properties_typeProperties_resourceGroupName')]",
					"mlWorkspaceName": "Sonntagsfrage-predictor",
					"servicePrincipalId": "[parameters('SonntagsfragePredictor_properties_typeProperties_servicePrincipalId')]",
					"servicePrincipalKey": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "sonntagsfrageKeys",
							"type": "LinkedServiceReference"
						},
						"secretName": "sp-aml-sonntagsfrage-clientSecret"
					},
