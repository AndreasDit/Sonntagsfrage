						},
						"linkedServiceName": {
							"referenceName": "SonntagsfragePredictor",
							"type": "LinkedServiceReference"
						}
					},
					{
						"name": "Send_preds_to_google_spreadsheet",
						"type": "AzureFunctionActivity",
						"dependsOn": [
							{
								"activity": "Sonntagsfrage-Forecaster-Pipeline",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"functionName": "google_spreadsheets_azure_func",
							"method": "POST",
							"headers": {},
							"body": "test"
						},
						"linkedServiceName": {
							"referenceName": "AzureFunction01",
							"type": "LinkedServiceReference"
						}
					}
				],
				"folder": {
					"name": "sonntagsfrage"
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureFunction01')]",
				"[concat(variables('factoryId'), '/linkedServices/SonntagsfragePredictor')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
