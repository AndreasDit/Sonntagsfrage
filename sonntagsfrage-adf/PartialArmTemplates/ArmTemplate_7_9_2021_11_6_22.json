					}
				],
				"folder": {
					"name": "sonntagsfrage"
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/SonntagsfragePredictor')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/SonntagsfragePredictor')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureMLService",
				"typeProperties": {
					"subscriptionId": "ffb77607-308f-4e35-baf6-a7ff61a55056",
					"resourceGroupName": "priv-proj-sonntagsfrage",
