					"servicePrincipalId": "2274a57e-306c-4a6e-9fc1-79cbde3781de",
					"servicePrincipalKey": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "sonntagsfrageKeys",
							"type": "LinkedServiceReference"
						},
						"secretName": "sp-aml-sonntagsfrage-clientSecret"
					},
					"tenant": "d5b090fb-04a6-4ddd-85cd-928023d29c36"
				}
			},
			"dependsOn": []
